   "source": [
    "# 336 C 5進法に変換、　数字は0, 2, 4, 6, 8\n",
    "n = int(input()) - 1\n",
    "m = []\n",
    "if n == 0:\n",
    "    print(0)\n",
    "while n > 0:\n",
    "    m.append((n % 5) * 2)\n",
    "    n = n // 5\n",
    "m.reverse()\n",
    "print(*m, sep = \"\")"
   ]
  }
